 <VirtualHost *:80>
     ServerName api.apmf.com
     DocumentRoot /var/www/backend
    
     <Directory /var/www/backend>
         Options Indexes FollowSymLinks MultiViews
         AllowOverride All
         Require all granted
     </Directory>

     ProxyPass / http://127.0.0.1:10000/
     ProxyPassReverse / http://127.0.0.1:10000/

 </VirtualHost>

